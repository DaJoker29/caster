{
  "name": "hercules",
  "version": "0.11.3",
  "description": "Modular CMS Platform",
  "main": "index.js",
  "repository": "git@github.com:DaJoker29/herc.git",
  "author": "Dewitt Buckingham <dj729@comcast.net>",
  "license": "MIT",
  "private": true,
  "engine": {
    "node": "~8.9.4"
  },
  "scripts": {
    "lint": "eslint --ext .js --ext .vue ./app/client && yarn stylelint app/**/*.scss",
    "fix": "eslint --fix --ext .js --ext .vue ./app/client && yarn stylelint --fix app/**/*.scss",
    "preversion": "git checkout master && git merge --no-ff develop -m 'Pull changes for release bump'",
    "postversion": "git push && git push --tags && git checkout develop && git merge --no-ff master -m 'Sync Develop branch' && git push",
    "preinstall": "node tools/pre-install",
    "postinstall": "node tools/post-install",
    "start": "nodemon index.js --config nodemon.json",
    "build": "webpack --config app/config/webpack.config.js",
    "dev": "grunt dev & pm2 start test-process.json",
    "prod": "grunt prod & pm2 start process.json",
    "access": "node tools/show-access-code",
    "cli": "node tools/cli",
    "test": "NODE_ENV=test mocha",
    "test-coverage": "NODE_ENV=test istanbul cover _mocha -- -R spec"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  },
