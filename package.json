{
  "name": "caster",
  "version": "0.0.0",
  "description": "A simple audio podcast manager and hosting",
  "main": "index.js",
  "repository": "git@github.com:DaJoker29/caster.git",
  "author": "Dewitt Buckingham <dj729@comcast.net>",
  "license": "MIT",
  "private": true,
  "engine": {
    "node": "~8.9.4"
  },
  "scripts": {
    "lint": "yarn eslint .",
    "fix": "yarn eslint-- fix .",
    "preversion": "git checkout master && git merge --no-ff develop -m 'Pull changes for release bump'",
    "postversion": "git push && git push --tags && git checkout develop && git merge --no-ff master -m 'Sync Develop branch' && git push",
    "start": "concurrently \"nodemon index.js --config nodemon.json\" \"grunt\"",
    "dev": "grunt dev & pm2 start process.json",
    "prod": "grunt prod & pm2 start test-process.json"
  },
  "dependencies": {
    "autoprefixer": "8.2.0",
    "babel-eslint": "8.2.2",
    "concurrently": "3.5.1",
    "cssnano": "3.10.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-last": "0.0.5",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-import-order-autofix": "0.8.3",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "grunt": "1.0.2",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-uglify": "3.3.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-eslint": "20.1.0",
    "grunt-postcss": "0.9.0",
    "grunt-sass": "2.1.0",
    "load-grunt-tasks": "3.5.2",
    "prettier": "1.11.1"
  }
}
